{
   "name": "raro-academy-boilerplate",
   "version": "0.0.1",
   "description": "Boilerplate para o projeto final da Raro Academy nodejs.",
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/body-parser": "^1.19.1",
      "@types/cors": "^2.8.12",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.13",
      "@types/faker": "^5.5.9",
      "@types/jest": "^27.5.2",
      "@types/jsonwebtoken": "^8.5.5",
      "@types/morgan": "^1.9.3",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.10.3",
      "@types/swagger-ui-express": "^4.1.3",
      "@types/yamljs": "^0.2.31",
      "@typescript-eslint/eslint-plugin": "^5.3.0",
      "@typescript-eslint/parser": "^5.3.0",
      "cross-env": "^7.0.3",
      "eslint": "^8.2.0",
      "jest": "^27.5.1",
      "ts-jest": "^27.0.7",
      "ts-node": "^10.9.1",
      "typescript": "^4.7.4"
   },
   "dependencies": {
      "@sendgrid/mail": "^7.7.0",
      "axios": "^0.22.0",
      "body-parser": "^1.19.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "cors": "^2.8.5",
      "dotenv": "^8.6.0",
      "express": "^4.17.1",
      "faker": "^5.5.3",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "mysql": "^2.18.1",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^4.5.0",
      "typedi": "^0.10.0",
      "typeorm": "0.2.38",
      "yamljs": "^0.3.0"
   },
   "scripts": {
      "build": "tsc",
      "start": "cross-env NODE_ENV=production node dist/index.js",
      "dev": "ts-node src/index.ts",
      "lint": "eslint . --ext .ts",
      "lint:fix": "npm run lint -- --fix",
      "test:coverage": "jest --coverage ",
      "test:watch": "jest --watchAll ",
      "test": "jest",
      "test:mac": "./node_modules/.bin/jest",
      "typeorm": "ts-node ./node_modules/.bin/typeorm",
      "typeorm:migrate": "typeorm migration:create -- -n",
      "typeorm:windows": "typeorm-ts-node-commonjs",
      "typeorm:win": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
   }
}
